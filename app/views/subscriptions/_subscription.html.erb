<div id="<%= dom_id subscription %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= subscription.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= subscription.user_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Status:</strong>
    <%= subscription.status %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Email:</strong>
    <%= subscription.email %>
  </p>

</div>
